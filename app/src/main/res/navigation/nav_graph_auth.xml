<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph2"
    app:startDestination="@id/Login2Fragment">

    <fragment
        android:id="@+id/Login2Fragment"
        android:name="com.blacklotus.makasibbasita.auth.login"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_Login2Fragment_to_Register2Fragment"
            app:destination="@id/Register2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/Register2Fragment"
        android:name="com.blacklotus.makasibbasita.auth.regisrerFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_regisrer">
        <action
            android:id="@+id/action_Register2Fragment_to_Login2Fragment"
            app:destination="@id/Login2Fragment" />
    </fragment>
</navigation>